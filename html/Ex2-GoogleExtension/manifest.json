{
    "manifest_version": 2,
    "name": "Habib Launcher",
    "description": "Quick launcher for HF apps",
    "version": "1.0.0",
    "icons": { "128": "icon-128.png" },
    "browser_action": {
        "default_icon": "icon-19.png",
        "default_popup": "popup.html"
    },
    "permissions": ["activeTab"]
}